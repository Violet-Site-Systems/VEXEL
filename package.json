{
  "name": "vexel",
  "version": "0.1.0",
  "description": "A DID Bridge Layer creating sovereign interoperability across distributed identity systems",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "node dist/database/migrate.js",
    "migrate:down": "node dist/database/migrate.js down",
    "example": "ts-node src/example.ts",
    "dev": "ts-node src/index.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "did",
    "identity",
    "blockchain",
    "decentralized",
    "polygon",
    "ipfs",
    "subgraph"
  ],
  "author": "Violet Site Systems",
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "dotenv": "^16.3.1",
    "ipfs-http-client": "^60.0.1",
    "kubo-rpc-client": "^3.0.2",
    "pg": "^8.11.3"
  }
